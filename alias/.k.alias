# k aliases
alias k="k --human --si"
alias ka="k --human --si -A"
alias kd="k --human --si -d"
alias kda="k --human --si -Ad"
alias kf="k --human --si -n"
alias kfa="k --human --si -An"
alias kt="k --human --si -t | awk 'NR != 1 && NR < 12'"
alias kta="k --human --si -At | awk 'NR != 1 && NR < 12'"
